package channel

type Request struct {
	Type string
	Seq  uint32
	Body any
}

type Response struct {
	ReqSeq uint32
	Err    string // TODO(bprosnitz) change this back to error when it is possible to do so. (issue 368)
	Body   any
}

type Message union {
	Request  Request
	Response Response
}
